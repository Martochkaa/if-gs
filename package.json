{
  "devDependencies": {
    "eslint": "^7.32.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^7.0.1",
    "jest": "^27.1.0",
    "prettier": "2.3.2"
  },
  "scripts": {
    "prepare": "husky install",
    "test": "jest"
  },
  "dependencies": {
    "lint-staged": "^11.1.2"
  },
  "lint-staged": {
    "*.(js|jsx)": [
      "npm run lint:write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
